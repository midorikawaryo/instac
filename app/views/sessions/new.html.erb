<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

<div class = login_form>
    <%= f.label :メールアドレス %>
    <%= f.email_field :email %>
</div>
<div class = login_form>
    <%= f.label :パスワード %>
    <%= f.password_field :password %>
</div>
    <%= f.submit "ログイン" %>
  <% end %>
</div>
